{
  "name": "tour-operator",
  "version": "1.4.5",
  "description": "Tour Operators for LSX",
  "main": "gulpfile.js",
  "scripts": {
    "test": "echo \"Error: no test specified\" && exit 1",
    "start": "cgb-scripts start",
    "build": "cgb-scripts build",
    "eject": "cgb-scripts eject",
    "build-pot": "wp i18n make-pot . languages/tour-operator.pot",
    "build-mopo": "rm -R languages/tour-operator-en_EN.mo && cp languages/tour-operator.pot languages/tour-operator-en_EN.po && wp i18n make-mo languages",
    "translate-US": "cp languages/tour-operator-en_EN.po languages/tour-operator-en_US.po && cp languages/tour-operator-en_EN.mo languages/tour-operator-en_US.mo"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/lightspeeddevelopment/tour-operator"
  },
  "keywords": [
    "lsx",
    "tours",
    "safaris",
    "accommodation"
  ],
  "author": "LightSpeed",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/lightspeeddevelopment/tour-operator/issues"
  },
  "homepage": "https://github.com/lightspeeddevelopment/tour-operator#readme",
  "devDependencies": {
    "gulp": "^4.0.2",
    "gulp-autoprefixer": "^8.0.0",
    "gulp-gettext": "^0.3.0",
    "gulp-jshint": "^2.1.0",
    "gulp-plumber": "^1.2.1",
    "gulp-rename": "^2.0.0",
    "gulp-rtlcss": "^2.0.0",
    "gulp-sass": "^5.0.0",
    "gulp-sort": "^2.0.0",
    "gulp-sourcemaps": "^3.0.0",
    "gulp-uglify": "^3.0.2",
    "gulp-util": "^3.0.8",
    "gulp-wp-pot": "^2.4.3",
    "jshint": "^2.12.0"
  },
  "dependencies": {
    "acorn": "^8.0.1",
    "cgb-scripts": "^1.23.1",
    "classnames": "^2.2.6",
    "clean-css": ">=4.2.3",
    "extend": "^3.0.2",
    "fstream": ">=1.0.12",
    "hoek": "^6.1.3",
    "lodash": "^4.17.20",
    "mem": "^9.0.0",
    "minimist": "^1.2.5",
    "npm": "^8.0.0",
    "runtask": "^1.0.0",
    "tar": ">=6.0.5"
  }
}
